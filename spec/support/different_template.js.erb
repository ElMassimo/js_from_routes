export default {
<% routes.each_with_index do |route, index| %>
<% if index > 0 %><%= "\n" %><% end
%>  <%= route.helper %>: options =>
    <% if route.path_only?
       %>asUrl(<% else
       %>request('<%= route.verb %>', <% end
                                    %>'<%= route.path %>', options),
<% end %>
}
